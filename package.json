{
  "name": "qa-hub",
  "version": "1.0.0",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "index.js",
  "scripts": {
    "test:run": "cypress run",
    "test:open": "cypress open",
    "test:ci-run": "cy2 run --parallel --record --key test --ci-build-id hello-cypress",
    "client": "cd client && npm start",
    "css:watch": "cd client && npm run watch:css",
    "server": "nodemon --signal SIGHUP index.js",
    "database": "mongod --dbpath ./data/db",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\" \"npm run css:watch\" \"npm run database\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jss4646/qa-hub.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jss4646/qa-hub/issues"
  },
  "homepage": "https://github.com/Jss4646/qa-hub#readme",
  "dependencies": {
    "@currents/cli": "^2.0.0",
    "@sentry/node": "^5.5.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cy2": "^2.0.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "img-diff-js": "^0.5.2",
    "mongodb": "^4.4.1",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.7",
    "pixelmatch": "^5.2.1",
    "prettier": "^2.4.1",
    "puppeteer": "^5.5.0",
    "puppeteer-cluster": "^0.22.0",
    "serve": "^11.3.2",
    "sharp": "^0.30.5",
    "sitemapper": "^3.1.10",
    "uuid": "^8.3.2",
    "winston": "^3.8.1",
    "ws": "^8.6.0"
  },
  "devDependencies": {
    "cypress": "^7.7.0",
    "cypress-fill-command": "^0.1.0"
  }
}
